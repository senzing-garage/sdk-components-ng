{
  "name": "@senzing/sdk-components-ng",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start:documentation": "./node_modules/.bin/compodoc -s -d docs",
    "start:docs": "npm run start:documentation",
    "build": "ng build @senzing/sdk-components-ng && npm run build:docs && node build-sdk.js",
    "build:docs": "./node_modules/.bin/compodoc -p projects/sdk-components-ng/tsconfig.lib.json -n \"Senzing SDK Components\" --includesName \"Guides\" -d docs -a projects/sdk-components-ng/documentation/images --includes projects/sdk-components-ng/docs --disableInternal --disablePrivate --disableDomTree --disableGraph --disableRoutesGraph",
    "clean": "rimraf dist/@senzing/sdk-components-ng",
    "package:sdk": "npm run build && cd ./dist/@senzing/sdk-components-ng/ && npm pack",
    "publish": "cd ./dist/@senzing/sdk-components-ng/ && npm publish --access restricted",
    "watch": "ng build @senzing/sdk-components-ng --watch",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/elements": "^7.0.2",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "core-js": "^2.5.4",
    "document-register-element": "1.8.1",
    "http-server": "^0.11.1",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "~7.0.4",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@compodoc/compodoc": "^1.1.7",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "concat": "^1.0.3",
    "concurrently": "^4.0.1",
    "fs-extra": "^7.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "node-sass": "^4.11.0",
    "protractor": "~5.4.0",
    "rimraf": "^2.6.2",
    "scss-bundle": "^2.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1",
    "wait-on": "^3.2.0"
  }
}
